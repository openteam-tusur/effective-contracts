<% unless @results.nil? %>
  <table>
    <thead>
      <tr>
        <th>Название</th>
        <th>Ответственный</th>
      </tr>
    </thead>
    <tbody>
      <ul>
        <% @factors.each do |factor|  %>
          <tr>
            <td class='center'><%= factor.title %>
            <% unless @authors.nil? %>
              <% @authors[factor.id].each do |a|%>
                <li><%= a.name %></li>
              <% end %>
            <% end %>
          </td>
          <td class= 'center'>
            <%=@users[factor.id].name unless @users[factor.id].nil? %>
          </td>
        </tr>
      <% end %>
    </ul>
  </tbody>
</table>
<% else %>
  Ничего не найдено
<% end %>

